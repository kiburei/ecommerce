<h5> Edit Category Name </h5>
<%= form_with(model: @category, local: true) do |form| %>
<div class="field">
<%= form.label :category_name %><br />
<%= form.text_field :category_name %>
</div>

   <div class="actions">
          <%= form.submit "Update", class: 'btn' %>
        </div>
<% end %>
